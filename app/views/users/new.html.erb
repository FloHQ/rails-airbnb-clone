<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<div class="container ">
  <%= simple_form_for [@user] do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%= f.input :phone_number %>
    <%= f.input :age %>
    <%= f.input :gender %>
    <%= f.submit "Create account", class: "btn btn-primary" %>
  <% end %>
</div>
